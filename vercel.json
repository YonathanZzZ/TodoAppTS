{
  "version": 2,
  "builds": [
    {
      "src": "client",
      "use": "@vercel/react",
      "config": {
        "outputDirectory": "public"
      }
    },
    {
      "src": "server.ts",
      "use": "@vercel/node",
      "config": {}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/public/index.html"
    }
  ]
}